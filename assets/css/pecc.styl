ModuleStyle(module, color, mod, CA)
	.unc-pecc-wrapper
		&.{module},
		.{module}
			[class^="icon-"].fill,
			[class*=" icon-"].fill,
			[class^="ti-"].fill,
			[class*=" ti-"].fill
				background-color color
				
			[class^="icon-"].outline,
			[class*=" icon-"].outline,
			[class^="ti-"].outline,
			[class*=" ti-"].outline
				color color
				border thin solid color

			[class^="pecc-"],
			[class*=" pecc-"]
				&.unc-fill
					background-color color
					
					&.unc-clickeable
						&:after
							border 2px solid color
			
			.unc-header
				background-color color

			h1,h2,h3,h4,h5,h6
				color color
				
				&.unc-row-title-line,
				&.unc-row-title-box
					background-color color
					
					&:before,
					&:after
						border-top 3px solid color
				
				&.unc-row-title-line
					background-color transparent
					color color
				
				&.unc-title-border
					border-color color
				
			.unc-card > h5:not(.unc-row-title-box):not(.unc-row-title-line)
				&:before
					background-color color
				small
					color color
			
			.unc-text-modal
				color color
			
			strong
				color color

			// Bootstrap Styles
			ul.nav-tabs
				> li.active > a
					color color !important
			
			.tabs .indicator
				background-color color
				
			.panel-group
				.panel-heading
					background-color color
					
			.unc-player
				.unc-player-header 
					.unc-header-bottom 
						.unc-header-title 
							small
								color CA
				.unc-player-list
					.unc-list-item
						&.active,
						&:hover
							background-color CA				

			// Custom Styles
			.unc-button
				background-color CA
				border thin solid CA
				
				&:hover
					background-color white
					color CA
			
			.unc-row-section-title
				background-image url('../img/' + mod + '_IMG_BG_FULL_V.png')
			
			.unc-row-border
				border 3px solid color
				color color
				
				&.unc-border-double
					&:after
						border-color color

			.unc-row-splash
				.unc-splash
					background-image url('../img/' + mod + '_IMG_SPLASH.png')
					
			.unc-row-degrant
				.unc-degrant-text
					background-color rgba(color,0.7)
					
			.unc-row-icon
				border-top 3px solid color
				border-bottom 3px solid color
				
			.unc-row-summary
				.unc-left
					background-image url('../img/' + mod + '_IMG01.jpg')
					
					.unc-summary-title
						background-image url('../img/' + mod + '_IMG02.png')
							
				.unc-right
					> h3
						background-color color
						
					.unc-summary-objectives
						ul
							li
								color color

					.unc-guests
						border thin dashed color
						
						.unc-autor							
							span
								background-color color
						
						h3
							background-color color
					

			.unc-row-bgfull
				h3,h4,h5
					&.unc-title-border
						&.unc-fill
							color color

			.unc-row-title-left
				border 2px solid color
					
			.unc-row-title-top
				border 2px solid color			
			
			.unc-row-prompt
				border-left 5px solid color
				
				.unc-prompt-title
					color color
			
			.unc-row-glosary		
				ul
					li
						strong
							background-color color !important
			
			.unc-row-icon-title
				span
					&.unc-fill
						&.unc-clickeable
							background-color color !important
					
			.unc-separator-dotted
				border-top-color rgba(color, 0.5)
			
			.unc-separator-solid
				border-top-color color
				
			.unc-line-left
				border-left-color color
				
			.unc-row-icon-side
				.unc-icon-side-border
					border 3px solid color
					
					&.unc-icon-left,
					&.unc-icon-right
						span
							color color
							
							&.unc-fill
								background-color color
			
			.unc-row-vt-list
				.unc-number
					background-color color
			
			.unc-row-card
				border 3px solid color
				
			.unc-row-icon-left
				[class*="pecc-"]
					color color
							
			.unc-triangle-top
				border-top-color color !important
			.unc-triangle-right
				border-right-color color !important
			.unc-triangle-bottom
				border-bottom-color color !important
			.unc-triangle-left
				border-left-color color !important
			
			.unc-collapse
				.unc-summary
						background-color color
							
