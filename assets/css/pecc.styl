/**
 * Font Icon PECC
 */
.unc-pecc-wrapper
	[class^="pecc-"],
	[class*=" pecc-"]
		font-family: 'pecc'
		speak: none
		font-style: normal
		font-weight: normal
		font-variant: normal
		text-transform: none
		line-height: 1
		-webkit-font-smoothing: antialiased
		-moz-osx-font-smoothing: grayscale
	
		&.unc-fill
			background-color m00
			border-radius 50%
			color white
			padding 2px
			
			&.unc-clickeable
				cursor pointer
				font-size 3em
				position relative

				&:after
					border 2px solid m00
					border-radius: 50%
					content ''
					display block
					height 60px
					left -4px
					padding 2px
					position absolute
					top -4px
					width 60px
					z-index: 2
	
