/**
 * Main Wrapper styles (Module00 Styles Default)
 */
.unc-pecc-wrapper
	background-color #fff
	color text
	// font-family 'Roboto', arial, sans_serif
	font-size 12pt
	margin 0 auto
	position relative

	[class^="icon-"].fill,
	[class*=" icon-"].fill,
	[class^="ti-"].fill,
	[class*=" ti-"].fill
		border-radius 50%
		color white
		display inline-block
		padding .5em
		
	[class^="icon-"].outline,
	[class*=" icon-"].outline,
	[class^="ti-"].outline,
	[class*=" ti-"].outline
		border-radius 50%
		display inline-block
		padding .5em
	
	h1,h2,h3,h4,h5,h6
		font-weight normal
		line-height 1.2em
		text-align center
		margin 12pt
		
		&.text-left
			text-align left
			
		&.text-right
			text-align right
			
		&.unc-row-title-line,
		&.unc-row-title-box
			color white
			display block
			margin 30pt auto
			max-width 300px
			text-transform uppercase
			
			&:before,
			&:after
				display inline-block
				content ""
				position absolute
				margin-top .5em
				width calc(50% - 150px)
				left 0
		
			&:after
				left auto
				right 0
				
		&.unc-row-title-line
			background-color transparent
			
		&.unc-row-title-box
			padding .5em
			
		&.unc-title-border
			margin-left auto
			margin-right auto
			padding .5em
			width 80%
			
	.unc-text-modal
		cursor pointer
		text-decoration underline
		
	h1
		font-size 3em
		text-transform uppercase
	h2
		color #aaa
		font-size 2em
		margin-top 2em
		text-transform uppercase
	h3
		font-size 1.5em
		text-transform uppercase
	h4
		font-size 2em
	h5
		font-size 1.5em
		
	.unc-card > h5:not(.unc-row-title-box):not(.unc-row-title-line)
			font-weight bold
			margin-top 3em
			padding-left 2.5em
			position relative
			text-align left
			text-transform uppercase
			
			&:before
				display block
				content ''
				height 28px
				margin-left -2.5em
				position absolute
				width 42px
			
			small
				font-size 1em
				font-weight bold
				text-transform initial
			
	h6
		font-size 1em
	
	img
		max-width 100%
		
	ul
		margin 0
		padding-left 1em
		width 100%
		
		li
			margin 1em 0
		
	// Bootstrap Styles
	.row
		margin 0
		
	[class*="col-"],
	[class^="col-"]
		padding 0
	
	ul.nav-tabs
		width 100% !important
	
		> li.active > a
			background-color transparent !important
			font-size 1.5em
			padding 0
	
	.panel 
		.panel-body
			padding 0
		
	.panel-group
		.panel-heading			
			h5
				color white
				font-weight normal
				margin 0
				text-align left
				text-transform initial
				
	// Parallax Vanilla Styles
	.pv-container
		position relative
		
	.pv-block-text
		color white
		left 50%
		position absolute
		text-align center
		top 50%
		transform translate3d(-50%,-50%,0)
		width 100%
		
		h2,h5
			color white
	
	.modal
		padding 0 !important
	
	.unc-row-tip
		text-align right
		padding 1em
		
		small
			color #999
			font-style italic
		
	.unc-player
		min-width auto

		.unc-header-title
			color white
			text-align left
			
			small
				overflow hidden
				text-overflow ellipsis
				white-space nowrap				
			
		.unc-player-list
			max-height calc(100vh - 255px)
			.unc-list-item
				.unc-list-title
					overflow hidden
					text-overflow ellipsis
					white-space nowrap
			
			.unc-audio-title
				font-family 'Brandon'
				font-size 1.2em
				font-weight bold
				margin 0
				text-align left

	// Custome Styles
	.unc-button
		border-radius 5px
		color white
		cursor pointer
		display inline-block
		font-size inherit
		font-weight normal
		margin 1em 0
		padding 5px 15px
		transition .3s all ease
		text-align center
		
		&:hover
			background-color white
		
		.btn-label.d-block
			display block
			margin 0
	
	.unc-flex-center
		align-items center
		display flex
		justify-content center
		
		.unc-number
			font-family 'Brandon'
			font-size 7em
